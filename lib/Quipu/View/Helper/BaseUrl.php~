class Quipu_View_Helper_BaseUrl extends  { 
  /**
   * 
   * @param type name desc
   * @uses Clase::methodo()
   * @return type desc
   */
  function baseUrl () { 
    $fc=Zend_Controller_front::getInstance();
    return $fc->getBaseUrl();
  } //end function
  
} //end class

?>